<Window x:Class="SearchWithMyBrowser.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchWithMyBrowser.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">

    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>
    
    <Grid>
        <TextBlock Text="{Binding CurrentSettings.EngineSelection}" />
        <TextBox Text="{Binding CurrentSettings.CustomURL, UpdateSourceTrigger=PropertyChanged}" Margin="0,20,0,0"/>
    </Grid>
</Window>
